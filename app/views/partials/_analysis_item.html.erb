<div class="analysis-item">
	<div class="analysis-header row">
		<div class="col-lg-9 col-md-9 col-sm-9">
			<h2><a href="http://www.gpw.pl/karta_spolki/<%= object['full_id'] %>/" target="_blank"><%= object['name'] %></a> <span class="tooltip-element" title="Usuń spółkę z listy"><span class="glyphicon glyphicon-minus-sign"></span></span></h2>
		</div>
		<div class="col-lg-3 col-md-3 col-sm-3">
			<select class="selectpicker" data-companyid="<%= object['company_id'] %>">
				<% object['dates'].each do |date| %>
				<option><%= date %></option>
				<% end %>
			</select>
		</div>
		<div class="clearfix "></div>
	</div>
	<%= render 'partials/analysis_item_body', :object => object %>
</div>

