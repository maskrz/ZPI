<div class= "calculator">
	<div class="panel panel-default ">
		<div class="panel-heading">
			KALKULATOR
		</div>
		<div class="panel-body">
			<div class="edit-place">
				<form role= "form">
					<div class= "form-group">
						<label>Wk≈Çad inwestycyjny:</label>
						<input type="text" class="form-control" id="contribution">
					</div>
					<div class= "form-group">
						<label>Zmiana kursu w %:</label>
						<input type="text" class="form-control" id="change-of-course">
					</div>
					<button type="button" class="btn btn-success">
						Oblicz
					</button>
				</form>
			</div>
			<div class= "count-place">
				<form role= "form">
					<div class= "form-group">
						<label>Prognozowany zysk:</label>
						<input type="text" class="form-control" id="profit" readonly>
					</div>
					<button type="count" class="btn btn-success">
						Nowe obliczenia
					</button>
				</form>
			</div>
		</div>
	</div>
</div>

